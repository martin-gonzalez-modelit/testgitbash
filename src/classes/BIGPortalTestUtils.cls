    /**
     * Get Application Update
     *
     * @param appIds a set of Applications Ids to filter by
     * @return List of Applications obtained from filter
     */
    public static List<Application__c> getApplicationsById(Set<Id> appIds) {
        List<Application__c> applications = new List<Application__c>();

        try {
            applications = [
                    SELECT Status__c, Building_Name__c, Building_Street_Address__c, Building_City__c, Building_State__c,
                        Building_ZIP_Code__c, Building_Type__c, Building_Vintage__c, Climate_Zone__c,
                        Override_Package_Type__c, Package_Type__c, Measure_Combination_Code__c, Measure_Combination__c,
                        Measure_Combination_Abbreviation__c, kWh_Savings__c, kW_Savings__c, Therms_Savings__c,
                        Incremental_Measure_Cost__c, kWh_Savings_2nd_BL__c, kW_Savings_2nd_BL__c, 
                        Therms_Savings_2nd_BL__c, Incremental_Measure_Cost_2nd_BL__c, Work_Start_Date__c, 
                        Work_Finish_Date__c, Test_Out_Date__c, Rebate_from_Tier__c, Application_Submission_Date__c, 
                        Test_In_Date__c, Total_Incentive_Adjustment__c, Total_Unit_Capacity__c, 
                        Project_Financing_Source__c, Project_Financing_Detail__c, Project_Financing_Description__c, 
                        Application_Step__c, Contractor__c, Unit__c, Unit__r.Name, Customer_Phone__c, 
                        Delivered_Sensible_EER__c, Delivered_Sensible_EER_Assessment_Date__c, 
                        Delivered_Sensible_EER_Scoresheet__c, HPHI_Practitioner__c,
                        (SELECT Rebate_Type__c, Measure_Code__c, Equipment_Type__c FROM Measures__r),
                        (SELECT Demand_Savings_kW__c, Has_HPHI__c, HPHI_Practitioner__c, HPHI_Bonus__c 
                        FROM Sub_Applications__r ORDER BY CreatedDate DESC LIMIT 1)
                    FROM Application__c
                    WHERE Id IN : appIds
                    ];
        } catch (QueryException e) {
            System.debug('Error: Couldn\'t get Measure_Combination_Code__mdt. Message: ' + e.getMessage());
        } catch (Exception e) {
            System.debug('Error: Couldn\'t get Measure_Combination_Code__mdt. Message: ' + e.getMessage());
        }

        return applications;
    }

