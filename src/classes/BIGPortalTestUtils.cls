    public static String getZipCodeWithClimateZoneAndCounty() {
        return [SELECT Zip_Code__c
                FROM Zip_Code__mdt
                WHERE Climate_Zone__c != NULL AND County__c != NULL
                LIMIT 1].Zip_Code__c;
    }

