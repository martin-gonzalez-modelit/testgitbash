    /**
     * Create measures based on an application list and measure types
     *
     * @param applications Application related to these measures
     * @param measureTypeList A list of Measure_Type__c to add to the new measures
     * @return List of measures created
     */
    public static List<Measure__c> createMeasures(List<Application__c> applications,
            List<Measure_Types__mdt> measureTypeList) {
        List<Measure__c> measureList = new List<Measure__c>();
        try {
            for (Application__c application : applications) {
                for (Measure_Types__mdt measureType : measureTypeList) {
                    Measure__c measure = new Measure__c(Application__c = application.Id);
                    MeasureUtils.setMeasure(measure, measureType);
                    measureList.Add(measure);
                }
            }
            insert measureList;
        } catch (DmlException e) {
            System.debug('Error: Couldn\'t create measure. Message: ' + e.getMessage());
        } catch (Exception e) {
            System.debug('Error: Couldn\'t create measure. Message: ' + e.getMessage());
        }

        return measureList;
    }

    /**
