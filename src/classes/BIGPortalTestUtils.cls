    /**
     * Looks for a determined error message in the error messages list.
     *
     * @param messages List of errors.
     * @param message The message to look for.
     * @return True if the message is found.
     */
    public static Boolean searchErrorMessage(List<Apexpages.Message> messages, String message) {
        Boolean containsMessage = false;
        for (Apexpages.Message msg : messages) {
            containsMessage = msg.getDetail().contains(message);
            if (containsMessage) {
                break;
            }
        }
        return containsMessage;
    }

    /**
     * Get measures savings.
     *
     * @param appIds Set of applications ids.
     * @return map of measures with (measure code + climate zone) as key.
     */
    public static Map<String, Measure__c> getMeasures(Set<Id> appIds) {
        Map<String, Measure__c> measures = new Map<String, Measure__c>();
        String climateZone = '';

        for (Measure__c measure : [
                SELECT Id, Measure_Code__c, kW_Savings__c, kWh_Savings__c, Therms_Savings__c, Application__c,
                    Application__r.Climate_Zone__c
                FROM Measure__c
                WHERE Application__r.Id IN : appIds]) {
            if (measure.Application__r.Climate_Zone__c != null) {
                climateZone = (String.valueOf(measure.Application__r.Climate_Zone__c)).leftPad(2, '0');
            }
            measures.put(measure.Measure_Code__c + climateZone, measure);
        }

        return measures;
    }

