    public static List<Measure_Types__mdt> getMeasureTypes(Set<String> recordNames) {
        List<Measure_Types__mdt> measureTypeList;
        try {
           measureTypeList = [
               SELECT Measure_Code_Abbreviation__c, Measure_Code__c, Qualifying_Measure_Code__c, Measure_Type__c,
                   Incentive__c, Measure_Description__c, Upgrade_Measure__c, developerName
               FROM Measure_Types__mdt
               WHERE DeveloperName IN :recordNames
               ];
        } catch (Exception e) {
            System.debug('Error: Couldn\'t retrieve measure type. Message: ' + e.getMessage());
        }
        return measureTypeList;
    }

