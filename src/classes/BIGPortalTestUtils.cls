    public static List<Zip_Code__mdt> getZipCodeData(List<String> zipCodes) {
        List<Zip_Code__mdt> zipCodeData = new List<Zip_Code__mdt>();
        // Create Zip Code Data
        try {
            zipCodeData = [
                SELECT Id, Climate_Zone__c, Zip_Code__c, PGE_Territory__c, County__c
                FROM Zip_Code__mdt
                WHERE Zip_Code__c IN: zipCodes
                ];
        } catch (Exception e) {
            System.debug('Couldn\'t get zip code data. Error: ' + e.getMessage());
        }

        return zipCodeData;
    }

    /**
     * Get a single zip code from the the Zip_Code__mdt that contains a climate zone and a county.
